
{% extends "base.twig" %}

{% block content %}

<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">No</th>
      <th scope="col">Nama</th>
      <th scope="col">NIM</th>
      <th scope="col">Jurusan</th>
    </tr>
  </thead>
  <tbody id="rowmaha">

{% for key,item in tr %}

    <tr>
      <td>{{ key+1 }}</td>
      <td>{{ item.user_nama }}</td>
      <td>{{ item.user_nim }}</td>
      <td>{{ item.user_jurusan }}</td>
    </tr>
{% endfor %}


  </tbody>
</table>

{% endblock %}